{
    "manifest_version": 3,
    "name": "PyScript Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension using PyScript.",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ]
}
